{
  "name": "corbium",
  "productName": "GDLauncher",
  "private": true,
  "version": "2.0.0",
  "description": "GDLauncher Carbon.",
  "author": "killpowa <davide@gdlauncher.com>",
  "license": "ARR",
  "scripts": {
    "prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || echo skipping pnpm install && npx pnpm build-web",
    "dev-app": "turbo run dev --filter=!@gd/web",
    "build-linux-x64": "turbo run build --filter=!@gd/web --filter=!@gd/core -- --x64 --linux",
    "build-win-x64": "turbo run build --filter=!@gd/web --filter=!@gd/core -- --x64 --win",
    "build-mac-x64": "turbo run build --filter=!@gd/web --filter=!@gd/core -- --x64 --mac",
    "build-mac-arm64": "turbo run build --filter=!@gd/web --filter=!@gd/core -- --arm64 --mac",
    "core-dev": "turbo run dev --filter @gd/core",
    "core-build": "turbo run build --filter @gd/core",
    "dev-web": "turbo run dev --filter=@gd/web",
    "build-web": "turbo run build --filter @gd/web"
  },
  "devDependencies": {
    "turbo": "^1.4.4"
  }
}
