{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@gd/desktop#dev": {
      "dependsOn": ["^@gd/core#dev", "^@gd/ui#build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "outputs": ["dist/**"]
    }
  }
}
