{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "core.node"],
      "cache": false
    },
    "@gd/desktop#build": {
      "dependsOn": ["^@gd/ui#build"],
      "cache": false
    },
    "@gd/desktop#dev": {
      "dependsOn": ["^@gd/core#dev", "^@gd/ui#build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "@gd/core": {
      "outputs": ["core.node"],
      "cache": false
    },
    "@gd/app_helper": {
      "cache": false
    },
    "dev": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    }
  },
  "globalDependencies": [".env"]
}
