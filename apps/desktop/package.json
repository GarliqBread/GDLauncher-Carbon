  "devDependencies": {
    "@aws-sdk/client-s3": "^3.216.0",
    "@gd/config": "workspace:*",
    "@gd/core": "workspace:*",
    "@gd/ui": "workspace:*",
    "@playwright/test": "^1.28.1",
    "@solidjs/router": "^0.5.1",
    "@typescript-eslint/parser": "^5.45.0",
    "bundlewatch": "^0.3.3",
    "chokidar": "^3.5.3",
    "custom-electron-titlebar": "^4.1.3",
    "electron": "21.3.1",
    "electron-builder": "^23.6.0",
    "electron-playwright-helpers": "^1.2.0",
    "electron-updater": "^5.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.9.1",
    "event-lite": "^0.1.2",
    "jsdom": "^20.0.3",
    "playwright": "^1.28.1",
    "posthog-js": "^1.36.0",
    "prettier": "^2.8.0",
    "solid-js": "^1.6.2",
    "solid-transition-group": "^0.0.12",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-solid": "^2.4.0"
