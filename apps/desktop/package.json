    "@solidjs/router": "^0.4.3",
    "@vitest/ui": "^0.24.0",
    "electron": "20.1.1",
    "electron-builder": "^23.3.3",
    "electron-playwright-helpers": "^1.2.0",
    "electron-updater": "^5.2.1",
    "event-lite": "^0.1.2",
    "jsdom": "^20.0.1",
    "logrocket": "3.0.1",
    "playwright": "^1.27.0",
    "solid-js": "^1.5.7",
    "solid-testing-library": "^0.3.0",
    "solid-transition-group": "^0.0.11",
    "typescript": "^4.8.4",
    "unocss": "^0.45.26",
    "vite": "^3.1.6",
    "vite-plugin-solid": "^2.3.9",
    "vitest": "^0.24.1"
