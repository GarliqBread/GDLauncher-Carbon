---
import HeadCommon from "@/components/HeadCommon.astro";
import Navbar from "@/components/Navbar/index.astro";
const url = new URL(Astro.request.url);
const currentPage = url.pathname;

---

<html>
<head>
  <HeadCommon />
</head>
<body class="font-main w-full">
  <Navbar {currentPage} />
  <main>
    <slot></slot>
  </main>
</body>

</html>